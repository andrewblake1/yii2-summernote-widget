{
	"name": "zelenin/yii2-summernote-widget",
	"description": "Yii2 Summernote widget. Super simple WYSIWYG editor on Bootstrap",
	"version": "0.0.2",
	"type": "yii2-extension",
	"keywords": ["yii2", "summernote", "widget", "wysiwyg"],
	"homepage": "https://github.com/zelenin/yii2-summernote-widget",
	"time": "2014-09-22",
	"license": "MIT",
	"authors": [
		{
			"name": "Aleksandr Zelenin",
			"email": "aleksandr@zelenin.me",
			"homepage": "http://zelenin.me",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/zelenin/yii2-summernote-widget/issues",
		"source": "https://github.com/zelenin/yii2-summernote-widget"
	},
	"require": {
		"php": ">=5.4.0",
		"yiisoft/yii2": "*",
		"bower-asset/fontawesome": "@stable",
		"bower-asset/codemirror": "@stable",
		"bower-asset/summernote": "@stable"
	},
	"autoload": {
		"psr-4": {
			"Zelenin\\yii\\widgets\\Summernote\\": ""
		}
	},
	"minimum-stability": "stable",
	"extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
	}
}